#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
	
    int n,l,sum;
    scanf("%d", &n);
    if(n>=10000 && n<=99999)
    {
        while(n>0)
        {
        l=n%10;
        sum=sum+l;
        n=n/10;
        }
        printf("%d",sum);    
    }   
    //Complete the code to calculate the sum of the five digits on n.
    return 0;
}
